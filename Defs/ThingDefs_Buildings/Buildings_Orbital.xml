<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="DoorBase">
		<defName>VacDoor</defName>
		<label>vac door</label>
		<description>An ultrasonic barrier that prevents depressurization in space. The field compresses air molecules near the threshold, preventing gases from escaping while allowing people to freely travel through it. A vac barrier will do nothing to prevent intruders.\n\nMust be powered to prevent depressurization.</description>
		<thingClass>OdysseyCompact.Building_VacDoor</thingClass>
		<statBases>
			<WorkToBuild>2000</WorkToBuild>
			<MaxHitPoints>230</MaxHitPoints>
			<Flammability>0</Flammability>
		</statBases>
		<rotatable>false</rotatable>
		<graphicData>
			<texPath>Things/Building/Door/Autodoor_Mover</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<enabled>false</enabled>
			</damageData>
			<!-- <texPath>Things/Building/VacBarrier/VacBarrier_Barrier</texPath> -->
			<!-- <graphicClass>Graphic_Multi</graphicClass> -->
			<!-- <drawSize>(1.5,1.5)</drawSize> -->
			<!-- <shaderType>TransparentPostLight</shaderType> -->
			<!-- <damageData> -->
				<!-- <enabled>false</enabled> -->
			<!-- </damageData> -->
			<!-- <color>(142,142,142)</color> -->
		</graphicData>
		<uiIconPath>Things/Building/Door/Autodoor_MenuIcon</uiIconPath>
		<!-- <uiIconPath>Things/Building/VacBarrier/VacBarrier_MenuIcon_south</uiIconPath> -->
		<costList>
			<Steel>80</Steel>
			<ComponentIndustrial>4</ComponentIndustrial>
			<GravlitePanel>2</GravlitePanel>
		</costList>
		<costStuffCount>25</costStuffCount>
		<useStuffTerrainAffordance>false</useStuffTerrainAffordance>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<useBlueprintGraphicAsGhost>true</useBlueprintGraphicAsGhost>
		<designationCategory>Odyssey</designationCategory>
		<building>
			<isAirtight>true</isAirtight>
			<isSupportDoor>true</isSupportDoor>
			<doorTopGraphic>
				<texPath>Things/Building/VacBarrier/VacBarrier_Top</texPath>
				<graphicClass>Graphic_Multi</graphicClass>
				<drawSize>(1.5,1.5)</drawSize>
			</doorTopGraphic>
			<blueprintClass>Blueprint_Door</blueprintClass>
			<blueprintGraphicData>
				<texPath>Things/Building/VacBarrier/VacBarrier_MenuIcon</texPath>
				<graphicClass>Graphic_Multi</graphicClass>
				<shaderType>EdgeDetect</shaderType>
				<drawSize>(1.5,1.5)</drawSize>
			</blueprintGraphicData>
		</building>
		<researchPrerequisites>
			<li>Autodoors</li>
			<li>OrbitalTech</li>
			<li>AdvancedGravtech</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>8</constructionSkillPrerequisite>
		<comps>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>100</basePowerConsumption>
			</li>
			<li Class="CompProperties_LowPowerUnlessVacuum">
				<lowPowerConsumptionFactor>0.55</lowPowerConsumptionFactor>
				<checkRoomVacuum>false</checkRoomVacuum>
			</li>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<!-- <designationHotKey>Misc1</designationHotKey> -->
		<uiOrder>2015</uiOrder>
		<building>
			<isStuffableAirtight>true</isStuffableAirtight>
		</building>
	</ThingDef>

